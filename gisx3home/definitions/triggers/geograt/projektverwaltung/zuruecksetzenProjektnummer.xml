<?xml version="1.0" encoding="UTF-8"?>
<CronEvent xmlns="http://www.geograt.de/namespaces/gisx3/triggerdefinition/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<enabled>true</enabled>
	<cronExpression>0 0 0 1 JAN ? *</cronExpression>
	<commands>
		<update>
			<partId>updateSequence</partId>
			<themeId>gisx3:configuration.sequence</themeId>
			<featureId>
				<constant>
					<string>geograt.projektverwaltung.projektnr</string>
				</constant>
			</featureId>
			<presets>
				<single attributeName="sequenceValue">
					<constant>
						<int>0</int>
					</constant>
				</single>
			</presets>
		</update>
	</commands>
</CronEvent>