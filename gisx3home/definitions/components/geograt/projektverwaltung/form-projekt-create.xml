<?xml version="1.0" encoding="UTF-8"?>
<Form xmlns="http://www.geograt.de/namespaces/gisx3/dialogdefinition/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<title>Daten zum Projekt</title>
	<content>
		<field>
			<name>Projektnummer</name>
			<attributeName>visibleId</attributeName>
			<href>
				<dashboard>
					<theme>
						<constant>
							<value>gisx3:geograt.projektverwaltung.projekt</value>
						</constant>
					</theme>
					<id>
						<self>
							<attribute>id</attribute>
						</self>
					</id>
				</dashboard>
			</href>
		</field>
		<field>
			<name>Projektstatus</name>
			<attributeName>textAttribute1</attributeName>
		</field>
		<field>
			<name>Projektname</name>
			<attributeName>textAttribute2</attributeName>
		</field>
		<field>
			<name>Verantwortlicher</name>
			<attributeName>textAttribute3</attributeName>
		</field>
		<field>
			<attributeName>doubleAttribute1</attributeName>
		</field>
		<field>
			<name>Beschreibung</name>
			<attributeName>textAttribute4</attributeName>
		</field>
		<field>
			<name>Fachbereich</name>
			<attributeName>multiSelectAttribute1</attributeName>
		</field>
		<field>
			<name>Durchf√ºhrungszeitraum</name>
			<content>
				<dateRange>
					<startAttributeName>dateAttribute1</startAttributeName>
					<endAttributeName>dateAttribute2</endAttributeName>
				</dateRange>
			</content>
		</field>
		<field>
			<attributeName>local::aufgabe</attributeName>
		</field>
	</content>
</Form>